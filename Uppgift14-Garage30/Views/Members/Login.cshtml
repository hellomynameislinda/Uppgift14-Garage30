@model Uppgift14_Garage30.Models.ViewModels.MemberLoginViewModel

@{
    ViewData["Title"] = "Login";
}

<h1>Login</h1>

<h4>Member</h4>
<hr />
<div class="row">
    <div class="col-md-4">

        <form asp-action="Login">
            <div class="form-group">
                <select asp-for="PersonalId" asp-items="ViewBag.Members">
                    <option disabled selected value="">--Choose Id Number--</option>
                </select>
                <span asp-validation-for="PersonalId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
